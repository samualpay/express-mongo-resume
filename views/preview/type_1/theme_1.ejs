<body>
	<!-- Page Preloder -->
	<div id="preloder">
		<div class="loader"></div>
	</div>
	
	<!-- Header section start -->
	<header class="header-section">
		<div class="container-fluid">
			<div class="row">
				<div class="col-md-4">
					<div class="site-logo">
						<h2><a href="#"><%= title %> </a></h2>
						<!-- <p>Enhance your online presence</p> -->
					</div>
				</div>
				<!-- <div class="col-md-8 text-md-right header-buttons">
					<a href="#" class="site-btn">Download CV</a>
					<a href="#" class="site-btn">Discover me</a>
				</div> -->
			</div>
		</div>
	</header>
	<!-- Header section end -->

	<!-- Hero section start -->
	<section class="hero-section spad">
		<div class="container-fluid">
			<div class="row">
				<div class="col-xl-10 offset-xl-1">
					<div class="row">
						<div class="col-lg-6">
							<div class="hero-text">
								<h2><%= name %> </h2>
								<p><%=  introduction%> </p>
								<!-- <h2>Maria Williams</h2>
								<p>I’m a digital designer in love with photography, painting and discovering new worlds and cultures.</p> -->
							</div>
							<div class="hero-info">
								<h2><%= info.title %> </h2>
								<!-- <h2>General Info</h2> -->
								<ul>
									<% info.list.forEach(elem => { %>
										<li><span><%= elem.label %> </span><%= elem.content %> </li>
									<% }) %>
									<!-- <li><span>Date of Birth</span>Aug 25, 1988</li>
									<li><span>Address</span>Rosia Road 55, Gibraltar, UK</li>
									<li><span>E-mail</span>mariawilliams@company.com</li>
									<li><span>Phone </span>+43 5266 22 345</li> -->
								</ul>
							</div>
						</div>
						<div class="col-lg-6">
							<figure class="hero-image">
								<img src="<%= heroImage %> " alt="5">
								<!-- <img src="/preview/type_1/img/hero.jpg" alt="5"> -->
							</figure>
					</div>
				</div>
			</div>
		</div>
	</section>
	<!-- Hero section end -->

	<!-- Social links section start -->
	<div class="social-section">
		<div class="container-fluid">
			<div class="row">
				<div class="col-xl-10 offset-xl-1">
					<div class="social-link-warp">
						<div class="social-links">
							<% socialLinks.forEach(elem => { %> 
								<a href="<%= elem.url  %> "><i class="fa fa-<%= elem.type %> "></i></a>
							<% }) %>
							<!-- <a href=""><i class="fa fa-pinterest"></i></a>
							<a href=""><i class="fa fa-linkedin"></i></a>
							<a href=""><i class="fa fa-instagram"></i></a>
							<a href=""><i class="fa fa-facebook"></i></a>
							<a href=""><i class="fa fa-twitter"></i></a> -->
						</div>
						<h2 class="hidden-md hidden-sm">My Social Profiles</h2>
					</div>
				</div>
			</div>
		</div>
	</div>	
	<!-- Social links section end -->

	<!-- Resume section start -->
	<section class="resume-section spad">
		<div class="container-fluid">
			<div class="row">
				<div class="col-xl-7 offset-xl-2">
					<div class="section-title">
						<h2>Work Experience</h2>
					</div>
					<ul class="resume-list">
						<% workExperienceList.forEach( elem => { %> 
						<li>
							<h2><%= elem.startAt %>-<%= elem.endAt %></h2>
							<h3><%= elem.company %></h3>
							<h4><%= elem.title %></h4>
							<p><%= elem.description %></p>
						</li>
						<% }) %> 
