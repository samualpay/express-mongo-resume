	<script src="/preview/type_1/js/owl.carousel.min.js"></script>
	<script src="/preview/type_1/js/magnific-popup.min.js"></script>
	<script src="/preview/type_1/js/circle-progress.min.js"></script>
	<script src="/preview/type_1/js/main.js"></script>
</body>
